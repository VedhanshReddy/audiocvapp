{
  "name": "AudioCV",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "WorksByCV",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^35.2.0",
    "electron-builder": "^24.6.0"
  },
  "dependencies": {
    "electron-log": "^5.3.4",
    "electron-updater": "^6.6.2",
    "fluent-ffmpeg": "^2.1.3"
  },
  "build": {
    "appId": "com.worksbycv.audiocv",
